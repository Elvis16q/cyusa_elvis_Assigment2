<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>project</display-name>
	<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>com.auca.service.HelloWord</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/login</url-pattern>
	</servlet-mapping>
  <servlet>
    <servlet-name>fileUploadServlet</servlet-name>
    <servlet-class>com.auca.service.StudentAdmission</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileUploadServlet</servlet-name>
    <url-pattern>/studentUploadServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>emailServlet</servlet-name>
    <servlet-class>com.auca.service.EmailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>emailServlet</servlet-name>
    <url-pattern>/sendConfirmationEmail</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>acceptanceServlet</servlet-name>
    <servlet-class>com.auca.service.AcceptanceServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>acceptanceServlet</servlet-name>
    <url-pattern>/acceptance</url-pattern>
  </servlet-mapping>
  
  <session-config>
  <session-timeout>7</session-timeout>
  </session-config>
</web-app>